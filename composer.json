{
    "name": "jamesh/laravel-uuid-cursor-pagination",
    "description": "An implementation of cursor pagination with UUID",
    "type": "plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "James Hemery",
            "email": "hello@james-hemery.pro"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php" : "^7.4",
        "illuminate/support": "^5.7|^5.8|^6.0",
        "illuminate/http": "^5.7|^5.8|^6.0",
        "illuminate/database": "^5.7|^5.8|^6.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "orchestra/testbench": "~3.7.0|~3.8.0"
    },
    "autoload": {
        "psr-4": {
            "Jamesh\\UuidCursorPagination\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jamesh\\UuidCursorPagination\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
